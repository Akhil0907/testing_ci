{
  "name": "example-basic",
  "version": "1.0.0",
  "description": "basic example how to run Cypress tests",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "test1": "node_modules\\.bin\\cypress run",
    "headTest": "npm run test1 -- --headed",
    "chromeTest": "npm run test1 -- --browser chrome",
    "smokeTest": "npm run test1 -- --spec cypress/integration/Smoke/**/*",
    "acceptanceTest": "npm run test1 -- --spec cypress/integration/Acceptance/**/*",
    "FunctionalTest": "npm run test1 -- --spec cypress/integration/Functional/**/*"

  },
  "author": "",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "cypress": "4.8.0"
  }
}
